<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@id/officeWifiNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
        android:layout_marginBottom="10sp"
        android:layout_marginTop="10sp"
        android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
        android:text="@string/office_wifi_no"
        android:visibility="gone" />

    <TextView
        android:id="@id/officeWifiSavedTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
        android:layout_marginBottom="10sp"
        android:layout_marginTop="10sp"
        android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
        android:text="@string/office_wifi_saved_title" />

    <TextView
        android:id="@id/officeWifiSaved"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
        android:layout_marginBottom="10sp"
        android:layout_marginTop="10sp"
        android:editable="true"
        android:enabled="false"
        android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" />

    <TextView
        android:id="@id/officeWifiCurrentTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
        android:layout_marginBottom="10sp"
        android:layout_marginTop="10sp"
        android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
        android:text="@string/office_wifi_current_title" />

    <TextView
        android:id="@id/officeWifiCurrent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
        android:layout_marginBottom="10sp"
        android:layout_marginTop="10sp"
        android:editable="true"
        android:enabled="false"
        android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" />
</LinearLayout>
